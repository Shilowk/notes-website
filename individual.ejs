<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
    <% results.forEach(function(note ) { %>
        <form action="/editNote/<%= note.id %>" method="post">
            <textarea name="title" id="title" cols="30" rows="2"><%= note.title%></textarea>
           <br>
          <textarea name="content" id="content" cols="30" rows="10"> <%= note.content %></textarea>
          <button type="submit">Edit</button>
        </form>

      


            
                
    <% }); %>
</body>
</html>